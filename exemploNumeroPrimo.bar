latinha numero;
latinha primo = 1;

canta("Digite um numero");
bebe("%d", &numero);

velho (numero <= 1){
    primo = 0;
}
barreiro {
    latinha i = 2;
    boateAzul (i; i <= (numero / 2); i++){
        velho ((numero % i) == 0){
            primo = 0;
            saidera;
        }
    }
}

velho (primo == 1){
    canta("O numero e primo");
}
barreiro{
    canta("O numero nao e primo");
}
